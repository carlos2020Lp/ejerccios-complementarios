Proceso NotaAlumno
	Definir a,b,c,d Como Entero ;
	definir e Como Real;
	Escribir "Ingrese nota 1 del alumno : ";
	leer a;
	Escribir "Ingrese nota 2 del alumno : ";
	leer b;
	Escribir "Ingrese nota 3 del alumno : ";
	leer c;
	e<-(a*0.20+b*0.30+c*50)/4;
	Escribir "EL PROMEDIO ES : ",e;
	
	
	
	
FinProceso
	